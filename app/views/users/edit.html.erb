<%= form_with model: @user, local:true do |f| %>
  <h4>プロフィール画像</h4>
  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%>
  <%= f.attachment_field :profile_image %>
  <h4>名前</h4>
  <%= f.text_field :name %>
  <h4>紹介文</h4>
  <%= f.text_area :introduction %>
  <%= f.submit '投稿' %>
<% end %>